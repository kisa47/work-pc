
# This file describes the network interfaces available on your system
# For more information, see netplan(5).
network:
  version: 2
  renderer: networkd
  ethernets:
    eno1:
      dhcp4: no
      addresses: [{{ ip_adress }}]
      nameservers:
        addresses: [{{ nameservers }}]
      routes:
        - to: default
          via: {{ gateway4 }}